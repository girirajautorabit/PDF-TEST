<?xml version="1.0" encoding="UTF-8"?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This is a Lightning Message Channel to request generation of an S-Doc.</description>
    <isExposed>true</isExposed>
    <lightningMessageFields>
        <description>Id for the base record of the generated S-Doc.</description>
        <fieldName>recordId</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>Status of the S-Doc generation process.</description>
        <fieldName>status</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>Id for the generated S-Doc.</description>
        <fieldName>fileId</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>Name for the generated S-Doc.</description>
        <fieldName>fileName</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>JSON format of the generated S-Doc.</description>
        <fieldName>result</fieldName>
    </lightningMessageFields>
    <masterLabel>DocGenResult</masterLabel>
</LightningMessageChannel>
