<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>60.0</apiVersion>
    <description>Case Flow with Description not equals to null</description>
    <environments>Default</environments>
    <interviewLabel>Case Flow with Description {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Case Flow with Description</label>
